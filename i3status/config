general { interval = 1 }

order += "volume master"
order += "battery 0"
order += "tztime local"

battery 0 { format = "%percentage" }
tztime local { format = "%e %b, %H:%M" }

volume master {
    format = "%volume"
    format_muted = "MUTED"
    device = "default"
    mixer = "Master"
    mixer_idx = 0
}
